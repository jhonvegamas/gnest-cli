  "repository": {
    "type": "git",
    "url": "https://github.com/jhonvegamas/gnest-cli.git"
  },
  "dependencies": {
    "commander": "^10.0.1",
    "figlet": "^1.6.0",
    "inquirer": "^9.1.5"
  },
  "devDependencies": {
    "prettier": "2.8.7"
  }
}
